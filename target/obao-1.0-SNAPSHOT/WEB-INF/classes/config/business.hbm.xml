<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.obao.entity">
    <class name="Business" table="t_business" >
        <id name="businessId" type="integer">
            <column name="business_id" />
            <generator class="native" />
        </id>
        <property name="photo" type="string" >
            <column name="photo" />
        </property>
        <property name="password" type="string" >
            <column name="password" />
        </property>
        <property name="email" type="string" >
            <column name="email" />
        </property>
        <property name="phone" type="string" >
            <column name="phone" />
        </property>
        <property name="address" type="string" >
            <column name="address" />
        </property>
        <property name="state" type="integer" >
            <column name="state" />
        </property>
        <property name="registerTime" type="date" >
            <column name="register_time" />
        </property>
        <property name="remark" type="string" >
            <column name="remark"  default="æ— "/>
        </property>
    <property name="name" type="string" >
        <column name="name" not-null="true" length="20"/>
    </property>
        <property name="domainId" type="integer" >
            <column name="domain_id" />
        </property>


    </class>
</hibernate-mapping>
