<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.obao.entity">
	<class name="Order" table="t_order">
		<id name="orderId" type="integer">
			<column name="order_id" />
			<generator class="native" />
		</id>
		<property name="productId" type="integer">
			<column name="product_id" />
		</property>
		<property name="userId" type="integer">
			<column name="user_id" />
		</property>
		<property name="orderNumber" type="string">
			<column name="order_number" />
		</property>
		<property name="code" type="string">
			<column name="code" />
		</property>
		<property name="number" type="integer">
			<column name="number" />
		</property>
		<property name="orderTime" type="timestamp">
			<column name="order_time" />
		</property>
		<property name="dealTime" type="timestamp">
			<column name="deal_time" />
		</property>
		<property name="finishTime" type="string">
			<column name="finish_time" />
		</property>
		<property name="state" type="integer">
			<column name="state"  default="0"/>
		</property>
		<property name="remark" type="string">
			<column name="remark" />
		</property>
		<property name="flavor" type="string">
			<column name="flavor" />
		</property>
		<property name="size" type="string">
			<column name="size" />
		</property>
	</class>
</hibernate-mapping>
